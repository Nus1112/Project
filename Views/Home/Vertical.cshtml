<script>
    // ฟังก์ชันสำหรับแสดงวันที่และเวลาปัจจุบัน
    function displayDateTime() {
        var now = new Date();
        var datetimeElement = document.getElementById("datetime");
        var day = now.getDate();
        var month = now.getMonth() + 1; // เริ่มต้นเดือนที่ 0 ดังนั้นต้องเพิ่ม 1 เพื่อให้เป็นเดือนจริง
        var year = now.getFullYear();
        var hours = now.getHours();
        var minutes = now.getMinutes();
        var seconds = now.getSeconds();

        // จัดรูปแบบให้สวยงาม เช่น 01/01/2022 10:30:45
        var formattedDateTime = day.toString().padStart(2, '0') + '/' + month.toString().padStart(2, '0') + '/' + year + ' ' +
            hours.toString().padStart(2, '0') + ':' + minutes.toString().padStart(2, '0') + ':' + seconds.toString().padStart(2, '0');
        datetimeElement.innerHTML = formattedDateTime;
    }

    // เรียกใช้ฟังก์ชันทุกๆ 1 วินาที (1000 milliseconds)
    setInterval(displayDateTime, 1000);

    // เรียกใช้ฟังก์ชันเมื่อหน้าเว็บโหลดเสร็จ
    displayDateTime();
</script>
